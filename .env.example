# Toggle the local admin panel and file writer middleware
VITE_ADMIN_ENABLED=false
# ADMIN_ENABLED is still read if you prefer keeping it separate.
ADMIN_ENABLED=false
# Optional local password gate for /admin (leave blank for no password)
VITE_ADMIN_PASSWORD=

# GitHub commit target used by Cloudflare Functions when saving admin changes
GITHUB_REPO=camo/mediahaus-print-power
GITHUB_BRANCH=main
# Personal access token with repo scope (only needed in production Functions, store securely)
GITHUB_TOKEN=
